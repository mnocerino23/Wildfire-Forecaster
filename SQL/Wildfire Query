SELECT OBJECTID, NWCG_REPORTING_UNIT_ID, 
NWCG_REPORTING_UNIT_NAME, SOURCE_REPORTING_UNIT,
SOURCE_REPORTING_UNIT_NAME, FIRE_NAME, FIRE_YEAR, 
DISCOVERY_DOY, CONT_DOY, STAT_CAUSE_DESCR, 
FIRE_SIZE, FIRE_SIZE_CLASS, LATITUDE, LONGITUDE, 
OWNER_DESCR, STATE, COUNTY, FIPS_NAME
FROM Fires WHERE FIRE_YEAR > 2000 AND STATE = 'CA';
